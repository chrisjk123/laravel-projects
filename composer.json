{
    "name": "chrisjk123/laravel-projects",
    "description": "Add projects database tables to a Laravel app",
    "keywords": [
        "laravelprojects",
        "laravel-projects"
    ],
    "homepage": "https://github.com/chrisjk123/laravel-projects",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Christopher Kelker",
            "email": "christopherjk123@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2",
        "laravel/framework": "^6.0|^7.0"
    },
    "require-dev": {
        "orchestra/testbench": "~3.8.0|^4.0|^5.0",
        "phpunit/phpunit": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Chriscreates\\Projects\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Chriscreates\\Projects\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Chriscreates\\Projects\\ProjectsServiceProvider"
            ]
        }
    }
}
